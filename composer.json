{
    "name": "deeplcom/deepl-php",
    "type": "library",
    "description": "Official DeepL API Client Library",
    "keywords": [
        "deepl",
        "translation",
        "translator",
        "api"
    ],
    "require": {
        "php": ">=7.3.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "ramsey/uuid": "^4.2",
        "squizlabs/php_codesniffer": "^3.3",
        "friendsofphp/php-cs-fixer": "^3",
        "php-mock/php-mock-phpunit": "^2.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DeepL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DeepL\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "DeepL SE",
            "email": "open-source@deepl.com"
        }
    ]
}
